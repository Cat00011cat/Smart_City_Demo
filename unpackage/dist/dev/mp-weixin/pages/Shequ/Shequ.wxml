<view><view class="topBanner"><swiper class="banner" autoplay="{{true}}" interval="2500"><block wx:for="{{bannerList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><swiper-item><view data-event-opts="{{[['tap',[['lookPropertyNotice',[index]]]]]}}" style="width:100%;height:100%;" bindtap="__e"><image class="bannerItem" src="{{item.image}}" mode="scaleToFill"></image><view class="propertyNotice">{{item.label}}</view></view></swiper-item></block></block></swiper></view><view style="display:flex;width:100%;"><block wx:for="{{serviceList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['jumpToService',['$0'],[[['serviceList','',index,'url']]]]]]]}}" style="width:25%;text-align:center;" bindtap="__e"><image style="width:96rpx;height:96rpx;" src="{{item.image}}" mode="aspectFit"></image><view>{{item.label}}</view></view></block></block></view><view><view style="font-size:48rpx;font-weight:bold;margin:24rpx;">商业推广</view><block wx:for="{{promoteList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['jumpToTuiguan',[index]]]]]}}" class="promoteItem" bindtap="__e"><image style="width:100%;" src="{{item.image}}"></image><text style="font-size:40rpx;">{{item.title}}</text></view></block></block></view></view>